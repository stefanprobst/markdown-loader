    "webpack": "^4.44.2"
  },
  "engines": {
    "node": ">=12.9",
    "yarn": "^1"
  },
  "commitlint": {
    "extends": [
      "@stefanprobst/commitlint-config"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@stefanprobst/eslint-config-node"
    ]
  },
  "husky": {
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
